{
  "functions": {
    "src/app/api/webhooks/stripe/route.ts": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.listora.ai"
        }
      ],
      "destination": "https://listora.ai/:path*",
      "permanent": true
    }
  ]
}
